                        android:id="@+id/text3"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:gravity="center"
                        android:background="@android:color/transparent"/>
                </LinearLayout>

                <View
                    android:layout_width="@dimen/mandarat_line_width"
                    android:layout_height="match_parent"
                    android:background="@color/colorMandaratLine"/>
            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/mandarat_line_width"
                android:background="@color/colorMandaratLine"/>
